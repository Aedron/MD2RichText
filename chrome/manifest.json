{
  "name": "MD2RichText",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Convert Markdown into Rich-text easily",
  "homepage_url": "https://github.com/HuQingyang/MD2RichText",
  "icons": {
    "48": "icons/favicon-48.png",
    "128": "icons/favicon-128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon128.png",
    "default_title": "MD2RichText"
  },
  "permissions": [

  ]
}
